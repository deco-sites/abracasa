{
  "font": {
    "fonts": [
      {
        "family": "Noto Sans",
        "variations": [
          { "weight": "300" },
          { "weight": "400" },
          { "weight": "500" },
          { "weight": "600" },
          { "weight": "700" }
        ]
      }
    ],
    "__resolveType": "website/loaders/fonts/googleFonts.ts"
  },
  "mode": "dark",
  "fonts": [{ "fontFamily": "Poppins" }],
  "mainColors": {
    "neutral": "#ffffff",
    "primary": "#ffffff",
    "warning": "#FFFFFF",
    "base-100": "#000000",
    "tertiary": "#5f9a99",
    "secondary": "#000000"
  },
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--animation-btn": "0.25s",
    "--btn-text-case": "normal-case",
    "--btn-focus-scale": "0.95"
  },
  "colorScheme": "any",
  "otherStyles": {
    "--tab-border": "1px",
    "--tab-radius": "0.5rem",
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s"
  },
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "complementaryColors": {
    "primary": { "primary-content": "#ffffff" },
    "base-200": "#e8e8e8",
    "base-300": "#ffffff",
    "base-content": "#ffffff",
    "primary-content": "#ffffff",
    "warning-content": "#555555",
    "secondary-content": "#ffffff"
  }
}
