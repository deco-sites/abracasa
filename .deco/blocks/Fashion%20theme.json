{
  "font": {
    "fonts": [
      {
        "family": "Noto Sans",
        "variations": [
          { "weight": "300", "italic": false },
          { "weight": "400", "italic": false },
          { "weight": "500", "italic": false },
          { "weight": "600", "italic": false },
          { "weight": "700", "italic": false }
        ]
      }
    ],
    "__resolveType": "website/loaders/fonts/googleFonts.ts"
  },
  "mode": "dark",
  "fonts": [{ "fontFamily": "Poppins" }],
  "mainColors": {
    "neutral": "#212121",
    "primary": "#000000",
    "success": "#F3F3F3",
    "warning": "#555555",
    "base-100": "#FFFFFF",
    "tertiary": "#5f9a99",
    "secondary": "#f3f3f3"
  },
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--animation-btn": "0.25s",
    "--btn-text-case": "normal-case",
    "--btn-focus-scale": "0.95"
  },
  "colorScheme": "any",
  "otherStyles": {
    "--tab-border": "1px",
    "--tab-radius": "0.5rem",
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s"
  },
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "complementaryColors": {
    "primary": { "primary-content": "#ffffff" },
    "base-200": "#e8e8e8",
    "base-300": "#7A7A7A",
    "warning-content": "#555555"
  }
}
